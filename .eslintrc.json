{
  "extends": ["next/core-web-vitals", "airbnb-base"],
  "rules": {
    "semi":"off",
    "implicit-arrow-linebreak": "off",
    "no-param-reassign": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
   ],
   "operator-linebreak": "off"
  }
}
